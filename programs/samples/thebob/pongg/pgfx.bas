DrawFONT:
RESTORE FontDATA
MaxWIDTH = 581
MaxDEPTH = 104
x = 0: y = 0

DO
    READ Count, Colr
    FOR Reps = 1 TO Count
        PSET (x, y), Colr
        x = x + 1
        IF x > MaxWIDTH THEN
            x = 0
            y = y + 1
        END IF
    NEXT Reps
LOOP UNTIL y > MaxDEPTH
RETURN

