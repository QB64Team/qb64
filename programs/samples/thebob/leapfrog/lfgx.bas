'Create images from DATA
MaxWIDTH = 300
MaxDEPTH = 395
x = 0: y = 0
RESTORE PixDATA
DO
    READ DataSTRING$
    FOR n = 1 TO LEN(DataSTRING$)
        Char$ = MID$(DataSTRING$, n, 1)
        IF Char$ = CHR$(225) THEN Char$ = CHR$(160)
        SELECT CASE Char$
            CASE "!"
                n = n + 1
                a$ = MID$(DataSTRING$, n, 1)
                Count = ASC(a$) + 68
            CASE "#"
                n = n + 1
                B$ = MID$(DataSTRING$, n)
                FOR i = 1 TO LEN(B$)
                    t$ = MID$(B$, i, 1)
                    IF t$ = "#" THEN EXIT FOR
                    c$ = c$ + t$
                NEXT i
                Count = VAL("&H" + c$)
                n = n + LEN(c$)
                c$ = ""
            CASE ELSE
                Count = ASC(Char$) - 60
        END SELECT
        n = n + 1
        Colr = VAL("&H" + MID$(DataSTRING$, n, 1))
        FOR Reps = 1 TO Count
            PSET (x, y), Colr
            x = x + 1
            IF x > MaxWIDTH THEN x = 0: y = y + 1
        NEXT Reps
    NEXT n
LOOP UNTIL y > MaxDEPTH 'DATA drawing loop ends here --------------------
'Draw mirror image of frog leaping frames
